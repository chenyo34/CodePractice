    "def largestAltitude(self, gain: List[int]) -> int:\n",
    "    max_alt = 0\n",
    "    cur_alt = 0\n",
    "    for i in range(len(gain)):\n",
    "            cur_alt += gain[i]\n",
    "            max_alt = max(max_alt, cur_alt)\n",
    "    return max_alt"
   ]
